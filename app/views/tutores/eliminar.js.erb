<% if @eliminado %>

   $("<div class='col-sm-12' id='msg-eliminar-tutor' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Eliminado exitosamente.</div>").insertBefore("#table-tutor");
   $("#form_buscar_tutor_id").val("<%=@tutor_elim.id%>")
   $("#form-buscar-tutor").submit();
    setTimeout(

      function(){ $("#msg-eliminar-tutor").remove(); 

       swal("Eliminado Exitosamente.");

    }, 1000);
   
    
<% else %>

  $("<div class='col-sm-12' id='msg-eliminar-tutor' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= j(@msg).html_safe %></div>").insertBefore("#table-tutor");
  setTimeout(function(){ 
    
    $("#msg-eliminar-tutor").remove();

    swal("No se pudo Eliminar. Intente m√°s tarde")

     }, 2000);
  	
<% end %>
