<% if @eliminado %>

   $("<div class='col-sm-12' id='msg-eliminar-profesional' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Eliminado exitosamente.</div>").insertBefore("#table-profesional");
   $("#form_buscar_profesional_id").val("<%=@profesional_elim.id%>")
   $("#form-buscar-profesional").submit();
    setTimeout(
      function(){ $("#msg-eliminar-profesional").remove(); 
       swal("Eliminado Exitosamente.");
    }, 1000);
   
    
<% else %>

  $("<div class='col-sm-12' id='msg-eliminar-profesional' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= j(@msg).html_safe %></div>").insertBefore("#table-profesional");
  setTimeout(function(){ 

    $("#msg-eliminar-profesional").remove(); 
  
    swal("No se pudo Eliminar. Intente m√°s tarde")

  }, 2000);
  	
<% end %>
