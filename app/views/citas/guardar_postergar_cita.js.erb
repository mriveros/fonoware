<% if @cita_ok %>

  $("<div class='col-sm-12' id='msg-agregar-cita' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Registrado Exitosamente.</div>").insertBefore("#form-modificar-cita");
   
  $("#form_buscar_citas_id").val('<%= @cita.id %>');
  $("#form_buscar_citas_fecha").val('<%= @cita.fecha_cita %>');
  $("#form-buscar-citas").submit();

  
  setTimeout(function(){ 
   
    swal("La Cita ha sido reagendada Exitosamente.");


  }, 2000);


<% else %>

  swal("Error al guardar:" + "<%=j(@msg)%>")
    
    $("<div class='col-sm-12' id='msg-agregar-cita' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= j(@msg) %></div>").insertBefore("#form-modificar-cita");

<% end %>
