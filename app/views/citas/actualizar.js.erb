<% if @precio_ok.present? %>

   $("<div class='col-sm-12' id='msg-editar-precio' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Actualizado Exitosamente.</div>").insertBefore("#form-editar-precio");
   
  $("#form_buscar_precios_id").val('<%= @precio.id %>');
  $("#form_buscar_precios_codigo").val('');
  $("#form_buscar_precios_monto").val('');
  $("#form_buscar_precios_descripcion").val('');
  setTimeout(function(){ 

    $("#<%= params[:window_id]%>").modal('hide'); 
    $("#form-buscar-precios").submit();
    swal("Guardado Exitosamente.");

  }, 3000);

<% else %>

  $("<div class='col-sm-12' id='msg-editar-precio' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= j(@msg) %></div>").insertBefore("#form-editar-precio");

<% end %>
