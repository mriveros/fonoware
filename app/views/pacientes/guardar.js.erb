<% if @guardado_ok %>

   $("<div class='col-sm-12' id='msg-agregar-pacientes' style='color:#000;padding-top:5px;font-size:10px;background:#d6ffd6;border-radius:5px;text-align:center;'>Registrado Exitosamente.</div>").insertBefore("#form-agregar-paciente");
   
  $("#form_buscar_pacientes_id").val('<%= @paciente.id %>');
  $("#form-buscar-pacientes").submit();
  swal("Guardado Exitosamente.")
  setTimeout(function(){ $("#<%= params[:window_id]%>").modal('hide'); }, 3000);


<% else %>
	swal("Error al guardar:" + "<%=j(@msg)%>")
    $("<div class='col-sm-12' id='msg-agregar-pacientes' style='color:#000;padding-top:5px;font-size:10px;background: #f8dfdf;border-radius:5px;text-align:center;'><%= j(@msg) %></div>").insertBefore("#form-agregar-paciente");

<% end %>
