<% if @valido %>

  <% if @imprimir %>

    swal("Datos Procesados!", "", "success");
    window.open("<%= url_for(produccion_planilla_resumen_produccion_cliente_pdf_path(@parametros))%>", '_blank')

  <% else %>
      
    $("#<%= params[:window_id]%>").html("<%= escape_javascript(render :partial => 'planilla_resumen_produccion_cliente')%>");
    $("#<%= params[:window_id]%>").modal('show');

  <% end %>

<% else %>
  
  swal("<%= @msg.html_safe %>", "", "error");

<% end %>