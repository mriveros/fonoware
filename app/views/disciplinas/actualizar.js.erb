<% if @disciplina_ok %>

  alert('<%= @msg %>');
    $('#<%= params[:window_id] %>').modal('hide');
    $("#form_buscar_disciplinas_id").val('<%= @disciplina.id %>');
    $("#form_buscar_disciplinas_descripcion").val('');
    $("#form-buscar-disciplinas").submit();
<% else %>

  alert('<%= @msg.gsub('.','\n') %>');

<% end %>


